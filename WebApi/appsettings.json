{
  "ProjectName": "Gallery",
  "Version" : "v1",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": "Host=;Port=;Database=;Username=;Password=;"
  },
  "Minio": {
    "ApiEndpoint": "",
    "BucketName": "gallery",
    "AccessKey": "",
    "SecretKey": "",
    "Secure": false
  },
  "UploadFileOptions":{
    "AvailableExtensions": [
      ".webp",
      ".png",
      ".jpeg",
      ".jpg",
      ".mp4",
      ".gif"
    ],
    "MaxSizeBytes": 268435456
  }
}
